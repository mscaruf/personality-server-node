{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "no-console": false,
        "no-var-requires": false,
        "object-literal-sort-keys": false,
        "interface-name": false,
        "member-access": [true, "no-public"],
        "ordered-imports": false
    },
    "rulesDirectory": []
}