{
  "name": "personality-server-node",
  "version": "1.0.0",
  "description": "Backend environment for Personality platform",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts, npm run dev:start",
    "start": "node .",
    "test": "mocha -r ts-node/register test/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mscaruf/personality-server-node.git"
  },
  "keywords": [
    "node",
    "mongo",
    "typescript",
    "express",
    "mocha",
    "chai"
  ],
  "author": "Marco Scaruffi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mscaruf/personality-server-node/issues"
  },
  "homepage": "https://github.com/mscaruf/personality-server-node#readme",
  "devDependencies": {
    "@types/chai": "^4.2.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/fs-extra": "^8.0.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.15",
    "@types/shelljs": "^0.8.6",
    "chai": "^4.2.0",
    "fs-extra": "^8.1.0",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "shelljs": "^0.8.3",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^5.7.14"
  }
}
